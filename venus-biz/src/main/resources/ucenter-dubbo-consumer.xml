<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo 
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:reference
            interface="com.tqmall.ucenter.service.shop.RpcShopService"
            id="rpcShopService" version="${dubbo.consumer.version}" check="false"/>
    <dubbo:reference
            interface="com.tqmall.ucenter.service.shop.RpcShopInfoService"
            id="rpcShopInfoService" version="${dubbo.consumer.version}" check="false"/>
    <dubbo:reference
            interface="com.tqmall.ucenter.service.account.RpcAccountService"
            id="rpcAccountService" version="${dubbo.consumer.version}" check="false"/>
    <dubbo:reference
            interface="com.tqmall.ucenter.service.address.RpcAddressService"
            id="rpcAddressService" version="${dubbo.consumer.version}" check="false"/>
    <dubbo:reference interface="com.tqmall.ucenter.service.shoptag.RpcShopTagService"
                     id="rpcShopTagService" version="${dubbo.consumer.version}" check="false"/>

</beans>