<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:reference id="fcPayOrderService"
        interface="com.tqmall.finance.service.order.FcPayOrderService"
        version="${dubbo.consumer.version}" />

    <dubbo:reference id="fcAccountCreditService"
        interface="com.tqmall.finance.service.credit.FcAccountCreditService"
        version="${dubbo.consumer.version}" />

    <dubbo:reference id="fcPayOrderCodService"
        interface="com.tqmall.finance.service.order.FcPayOrderCodService"
        version="${dubbo.consumer.version}" />
    <dubbo:reference id="fcPayOrderOnLineService"
                     interface="com.tqmall.finance.service.order.FcPayOrderOnLineService"
                     version="${dubbo.consumer.version}" />

</beans>